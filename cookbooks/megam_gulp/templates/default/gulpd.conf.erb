use-tls: false
tls-cert-file: /path/to/cert.pem
tls-key-file: /path/to/key.pem
name: <%= node.name %>
riak:
  url: api.megam.co:8087
  bucket: appreqs
scm:
  project: <%= node["gulp"]["project_name"] %>
  builder: <%= node["gulp"]["builder"] %>
  local_repo: <%= node["gulp"]["local_repo"] %>
  remote_repo: <%= node["gulp"]["remote_repo"] %>
  bucket-support: false
aws:
  access-key-id: funky_screwy_id
  secret-access-key: secret-very-secret
  s3:
    region-name: ap-southeast-1
    endpoint: s3-ap-southeast-1.amazonaws.com
    bucketEndpoint: s3-ap-southeast-1.amazonaws.com
    location-constraint: true
    lowercase-bucket: true
api:
  server: https://api.megam.co
  email: <%= node["gulp"]["email"] %>
  api_key: <%= node["gulp"]["api_key"] %>
amqp:
  url: amqp://guest:guest@rabbitmq1.megam.co.in:5672/
  exchange: megam_<%= node.name %>_exchange
  queue: megam_<%= node.name %>_queue
  consumerTag: megam_node_consumer
  routingkey: megam_key
