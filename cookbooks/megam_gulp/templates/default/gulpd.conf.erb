use-tls: false
tls-cert-file: /path/to/cert.pem
tls-key-file: /path/to/key.pem
MEGAM_HOME: /var/lib/megam/
update_queue: <%= node.name.split(".").first %>
docker_queue: Docker.<%= node.name %>
id: <%= node["gulp"]["asm_id"] %>
name: <%= node.name %>

etcd:
  path: http://<%= node['megam_etcd']%>:4001
  networkpath: <%= node['megam_etcd']%>:4001
  directory: megam 

geard:
  host: localhost:43273

riak:
  url: <%= node['megam_riak'] %>:8087
  bucket: appreqs
scm:
  project: <%= node["gulp"]["project_name"] %>
  builder: <%= node["gulp"]["builder"] %>
  local_repo: <%= node["gulp"]["local_repo"] %>
  remote_repo: <%= node["gulp"]["remote_repo"] %>
  bucket-support: false

api:
  server: https://<%= node['megam_riak'] %>
  email: <%= node["gulp"]["email"] %>
  api_key: <%= node["gulp"]["api_key"] %>
amqp:
  url: amqp://guest:guest@<%= node['rabbitmq_host'] %>:5672/
  exchange: megam_<%= node.name %>_exchange
  queue: megam_<%= node.name %>_queue
  consumerTag: megam_node_consumer
  routingkey: megam_key

