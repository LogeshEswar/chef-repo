input {

  file {
    type => "access-log"
    path => [ "/usr/local/share/megamherk/logs/*/*" ]
  }
}


filter {
  grok {
    type => "access-log"
    match => [ "@source_path", "(//usr/local/share/megamherk/logs/)(?<source_key>.+)(//*)" ]
  }
}

output {
  stdout { debug => true debug_format => "json"}
  redis {
    key => '%{source_key}'
    type => "access-log"
     data_type => "channel"
     host => "redis1.megam.co.in"
  }
}
