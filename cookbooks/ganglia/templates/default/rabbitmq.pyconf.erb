#Rabbitmq pyconf
 
modules {
  module {
    name = "rabbitmq"
    language = "python"
 
    param metric_group {
      value = "rabbitmq"
    }
    param host {
      value = "localhost"
    }
    param port {
      value = "15672"
    }
 
    param vhost {
      value = "/"
    }
    param username {
      value = "guest"
    }
    param password {
      value = "guest"
    } 
  }
}  
 
collection_group {
  collect_every = 30
  time_threshold = 60

  metric {
    name_match = "rmq_(.+)"
  }
}
